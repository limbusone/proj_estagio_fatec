<h1>Listing concedentes</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Cnpj</th>
    <th>Telefone</th>
    <th>Email</th>
    <th>Cep</th>
    <th>Logradouro</th>
    <th>Numero</th>
    <th>Bairro</th>
    <th>Cidade</th>
    <th>Estado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @concedentes.each do |concedente| %>
  <tr>
    <td><%= concedente.nome %></td>
    <td><%= concedente.cnpj %></td>
    <td><%= concedente.telefone %></td>
    <td><%= concedente.email %></td>
    <td><%= concedente.endereco.cep %></td>
    <td><%= concedente.endereco.logradouro %></td>
    <td><%= concedente.endereco.numero %></td>
    <td><%= concedente.endereco.bairro %></td>
    <td><%= concedente.endereco.cidade %></td>
    <td><%= concedente.endereco.estado %></td>
    <td><%= link_to 'Show', concedente %></td>
    <td><%= link_to 'Edit', edit_concedente_path(concedente) %></td>
    <td><%= link_to 'Destroy', concedente, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Concedente', new_concedente_path %>
