  <div class="fields well" <%= (dome) ? "style='display : block'" : "style='display : none'"%>>
    <input type="hidden" value="<%= (dome) ? 1 : 0%>" name="campos_dia_semana[]" />
    <div class="f">
      <input type="hidden" name="hidHoraDiaId[domingo][]" value="<%= (dome) ? dom.first.id : nil%>" />    
      <%= label_tag "dias_semana[domingo][inicio][]", "Inicio:" %>
      <%= select_time (dome) ? Time.new(DateTime.now.year, 
      DateTime.now.month, DateTime.now.day, dom.first.inicio.hour, dom.first.inicio.min) : Time.now, 
      options = { :prefix => "dias_semana[domingo][inicio][]" }, 
      html_options = { :class => "selecting2", :tipo => "inicio" } %>
      <%= label_tag "dias_semana[domingo][final][]", "Final:" %>
      <%= select_time (dome) ? Time.new(DateTime.now.year, 
      DateTime.now.month, DateTime.now.day, dom.first.final.hour, dom.first.final.min) : Time.now, 
      options = { :prefix => "dias_semana[domingo][final][]" }, 
      html_options = { :class => "selecting2", :tipo => "final" } %>
    </div>
    <div class="newFields">
      <% if (dom.length > 1)%>
        <% for i in 1...dom.length do%>
          <div class="f">
            <br />
            <input type="hidden" name="hidHoraDiaId[domingo][]" value="<%= dom[i].id%>" />    
            <%= label_tag "dias_semana[domingo][inicio][]", "Inicio:" %>
            <%= select_time Time.new(DateTime.now.year, 
            DateTime.now.month, DateTime.now.day, dom[i].inicio.hour, dom[i].inicio.min), 
            options = { :prefix => "dias_semana[domingo][inicio][]" }, 
            html_options = { :class => "selecting2", :tipo => "inicio" } %>
            <%= label_tag "dias_semana[domingo][final][]", "Final:" %>
            <%= select_time Time.new(DateTime.now.year, 
            DateTime.now.month, DateTime.now.day, dom[i].final.hour, dom[i].final.min), 
            options = { :prefix => "dias_semana[domingo][final][]" }, 
            html_options = { :class => "selecting2", :tipo => "final" } %>            
          </div>
        <% end%>
      <% end%>      
    </div>
  </div>
  <br />
  <div class="fields">
    <div class="control-group">
      <%= label_tag "carga_horaria", "Carga horária:" %>
      <div class="controls">
        <%= text_field_tag "carga_horaria", @detalhe_termo.carga_horaria_semanal, 
        :class => "uneditable-input", :readonly => "readonly" %>
        <a class="btn btn-small cmdCalcularCargaHoraria">
          <i class="icon-check"></i>
          Calcular
        </a>    
      </div>
    </div>
  </div>
  <input type="hidden" name="hidConclusao" id="hidConclusao" value="0" />
  <div class="form-actions">
    <button type="submit" class="btn cmdConclusao btn-primary" name="commit">
      <i class="icon-ok"></i>
      Enviar
    </button>      
  </div>
<% end %>
